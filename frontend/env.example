# Frontend Environment Variables
# Copy this file to .env.local for local development

# Development API URL (change to match your local ElizaOS)
VITE_API_URL=http://localhost:3000

# Development API Key (leave empty for local development)
VITE_API_KEY=

# Maintenance Mode - Set to 'true' to enable maintenance page
# This will block access to all routes and show maintenance message
VITE_MAINTENANCE_MODE=false

# Contract Address - The blockchain contract address to display in header
VITE_CONTRACT_ADDRESS=0x1234567890abcdef1234567890abcdef12345678

# X (Twitter) Username - The username to link to on maintenance page (without @)
VITE_X_USERNAME=futurepurl

# High Traffic Alert - Set to 'true' to show traffic warning banner
VITE_SHOW_TRAFFIC_ALERT=false

# Traffic Alert Message - Custom message for high traffic situations
VITE_TRAFFIC_ALERT_MESSAGE=Experiencing high traffic - response times may be slower than usual

# For production deployment, use:
# VITE_API_URL=https://your-elizaos-api.railway.app
# VITE_API_KEY=your-production-api-key
# VITE_MAINTENANCE_MODE=false
# VITE_CONTRACT_ADDRESS=your-actual-contract-address
